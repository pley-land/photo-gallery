    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "express": "^4.16.3",
    "jest": "^23.4.1",
    "jquery": "^3.3.1",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-image-lightbox": "^5.0.0",
    "react-modal": "^3.5.1",
